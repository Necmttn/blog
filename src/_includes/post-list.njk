<ol reversed>
{% for post in postList | reverse %}
  <li class="article-list-item-container">
    <span class='article-date'>{{ post.date | readableDate }}</span>
    <h3>
      <a href="{{ post.url }}">{{ post.data.title }}</a>
    </h3>
    {{ post.data.page.excerpt | safe }}
    <div class="article-list-actions">
      <a href="{{ post.url }}">Read more</a>
    </div>
  </li>
{% endfor %}
</ol>